{
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-unused-vars": 0,
      "no-console": 0,
      "no-mixed-spaces-and-tabs": 0
    }
  }
}